<html>
    <head>
        <title><%= title%></title>
        <link rel="stylesheet" style="" href="/stylesheets/styles.css">
        <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
        <script src="/javascripts/todo-list.js"></script>
    </head>
    <body>
        <h1>My Todo List</h1>
        <div id="todo-list">
            <form action="">
                <input type="text" name="item" placeholder="Add new item.." required>
                <button type="submit">Add Item</button>
            </form>
            <ul>
                <%todos.forEach(function (data) { %>
                <li><%= data.item%></li>
                <%})%>
            </ul>
        </div>
    </body>
</html>